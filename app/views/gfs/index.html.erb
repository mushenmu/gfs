<p id="notice"><%= notice %></p>

<h1>Gfs</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gfs.each do |gf| %>
      <tr>
        <td><%= gf.url %></td>
        <td><%= gf.state %></td>
        <td><%= link_to 'Show', gf %></td>
        <td><%= link_to 'Edit', edit_gf_path(gf) %></td>
        <td><%= link_to 'Destroy', gf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gf', new_gf_path %>
