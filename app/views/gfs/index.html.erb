<p id="notice"><%= notice %></p>

<h1>Gfs<small> &nbsp;<%= @data.size %></small></h1>
<table>
  <tbody>
  <tr>
    <%= search_form_for @q do |f| %>
    <td>
      <%= f.label :state %>
      <%= f.search_field :state_eq %>
    </td>
  
    <td>
      <%= f.label :"检索" %>
      <%= f.submit %>
    </td>
    <% end %>
  </tr>
  </tbody>
  </table>


<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gfs.each do |gf| %>
      <tr>
        <td><%= gf.url %></td>
        <td><%= gf.state %></td>
        <td><%= link_to 'Show', gf %></td>
        <td><%= link_to 'Edit', edit_gf_path(gf) %></td>
        <td><%= link_to 'Destroy', gf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gf', new_gf_path %>


<%= will_paginate @gfs %>